<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0" prefWidth="350.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.uady.reportecsv.LogInController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TextField fx:id="userField" promptText="Usuario" />
    <PasswordField fx:id="passwordField" promptText="Contraseña" />
    <HBox alignment="CENTER_RIGHT">
        <Label fx:id="signIn" alignment="CENTER_RIGHT" contentDisplay="CENTER" onMouseClicked="#onSignInClick" text="Crear cuenta" textAlignment="RIGHT" textFill="#0f0be1" underline="true" />
    </HBox>
    <HBox alignment="CENTER">
        <Button id="logInButton" mnemonicParsing="false" onAction="#onLogInAction" text="Iniciar Sesión" />
    </HBox>
</VBox>
